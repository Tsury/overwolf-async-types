{
  "version": "1.0.4",
  "name": "overwolf-async-types",
  "author": "Amit Tsur",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tsury/overwolf-async-types.git"
  },
  "type": "module",
  "types": "index.d.ts",
  "module": "./promisify.js",
  "scripts": {
    "build": "npm run fetch && node generateAsyncDefinitions.js",
    "fetch": "node fetchFiles.cjs",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ts-morph": "^19.0.0"
  },
  "bin": {
    "generate-overwolf-async-types": "./rebuild.cjs"
  },
  "devDependencies": {
    "diff": "^8.0.2",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/exec": "^6.0.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/exec",
        "cmd": "npm version ${nextRelease.version} --workspaces-update=false --no-git-tag-version --allow-same-version"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npm publish --provenance --access public"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
